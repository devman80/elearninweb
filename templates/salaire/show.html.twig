{% extends 'base.html.twig' %}

{% block title %}Salaire{% endblock %}

{% block body %}
    <h1>Salaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ salaire.id }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ salaire.montant }}</td>
            </tr>
            <tr>
                <th>Datesalaire</th>
                <td>{{ salaire.datesalaire ? salaire.datesalaire|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ salaire.createdAt ? salaire.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedFromIp</th>
                <td>{{ salaire.createdFromIp }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ salaire.updatedAt ? salaire.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedFromIp</th>
                <td>{{ salaire.updatedFromIp }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ salaire.deletedAt ? salaire.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DeletedFromIp</th>
                <td>{{ salaire.deletedFromIp }}</td>
            </tr>
            <tr>
                <th>Editable</th>
                <td>{{ salaire.editable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_salaire_index') }}">back to list</a>

    <a href="{{ path('app_salaire_edit', {'id': salaire.id}) }}">edit</a>

    {{ include('salaire/_delete_form.html.twig') }}
{% endblock %}
