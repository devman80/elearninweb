{% extends 'base.html.twig' %}

{% block title %}Inscrire index{% endblock %}

{% block body %}
    <h1>Inscrire index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Dateinscrire</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inscrire in inscrires %}
            <tr>
                <td>{{ inscrire.id }}</td>
                <td>{{ inscrire.dateinscrire ? inscrire.dateinscrire|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_inscrire_show', {'id': inscrire.id}) }}">show</a>
                    <a href="{{ path('app_inscrire_edit', {'id': inscrire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_inscrire_new') }}">Create new</a>
{% endblock %}
