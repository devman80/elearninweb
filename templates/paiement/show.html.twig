{% extends 'base.html.twig' %}

{% block title %}Paiement{% endblock %}

{% block body %}
    <h1>Paiement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ paiement.id }}</td>
            </tr>
            <tr>
                <th>Modepaiement</th>
                <td>{{ paiement.modepaiement }}</td>
            </tr>
            <tr>
                <th>Montantpaiement</th>
                <td>{{ paiement.montantpaiement }}</td>
            </tr>
            <tr>
