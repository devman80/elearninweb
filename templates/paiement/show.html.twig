            <tr>
                <th>DeletedFromIp</th>
                <td>{{ paiement.deletedFromIp }}</td>
            </tr>
            <tr>
                <th>Editable</th>
                <td>{{ paiement.editable ? 'Yes' : 'No' }}</td>
